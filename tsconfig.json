{
    "compilerOptions": {
        "outDir": "./js",
        "module": "ESNext",
        "target": "ESNext",
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "strictPropertyInitialization": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "removeComments": true
    },
    "include": ["./ts/*.ts"],
    "exclude": ["./js/*.js"]
}
